[browser/feature/tor-launcher@torproject.org] torlauncher.jar:
  components/     (src/components/*)

% content  torlauncher %content/
  content/        (src/chrome/content/*)

% locale   torlauncher @AB_CD@ %locale/@AB_CD@/
  locale/@AB_CD@/ (src/chrome/locale/@AB_CD@/*)

  modules/        (src/modules/*)

% resource torlauncher  %

% skin     torlauncher  default %skin/
  skin/           (src/chrome/skin/*)

